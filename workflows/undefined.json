    {
      "parameters": {
        "functionCode": "// Extract Udemy image URL from Discudemy page HTML\nconst items = $input.all();\n\nreturn items.map(item => {\n  const courseData = item.json;\n  const html = item.json.data || ''; // HTML from the HTTP request\n  \n  let udemyImageUrl = null;\n  \n  // Pattern 1: Look for Udemy CDN images directly\n  const udemyCdnPatterns = [\n    /https:\\/\\/img-c\\.udemycdn\\.com\\/course\\/[^\"'\\s>]+/gi,\n    /https:\\/\\/img-b\\.udemycdn\\.com\\/course\\/[^\"'\\s>]+/gi,\n    /https:\\/\\/udemy-images\\.udemy-com\\/course\\/[^\"'\\s>]+/gi\n  ];\n  \n  // Pattern 2: Look for og:image meta tag with Udemy image\n  const metaPatterns = [\n    /<meta[^>]*property=\"og:image\"[^>]*content=\"([^\"]*udemy[^\"]+)\"/gi,\n    /<meta[^>]*name=\"twitter:image\"[^>]*content=\"([^\"]*udemy[^\"]+)\"/gi\n  ];\n  \n  // Pattern 3: Look for data-src or src attributes with Udemy images\n  const imgPatterns = [\n    /<img[^>]*src=\"([^\"]*udemy[^\"]*\\.(jpg|jpeg|png|webp))\"[^>]*>/gi,\n    /<img[^>]*data-src=\"([^\"]*udemy[^\"]*\\.(jpg|jpeg|png|webp))\"[^>]*>/gi,\n    /<img[^>]*data-lazy=\"([^\"]*udemy[^\"]*\\.(jpg|jpeg|png|webp))\"[^>]*>/gi\n  ];\n  \n  // Try Udemy CDN patterns first (most reliable)\n  for (const pattern of udemyCdnPatterns) {\n    const matches = [...html.matchAll(pattern)];\n    if (matches.length > 0) {\n      udemyImageUrl = matches[0][0];\n      break;\n    }\n  }\n  \n  // Try meta tags\n  if (!udemyImageUrl) {\n    for (const pattern of metaPatterns) {\n      const matches = [...html.matchAll(pattern)];\n      if (matches.length > 0) {\n        udemyImageUrl = matches[0][1];\n        break;\n      }\n    }\n  }\n  \n  // Try image tags\n  if (!udemyImageUrl) {\n    for (const pattern of imgPatterns) {\n      const matches = [...html.matchAll(pattern)];\n      if (matches.length > 0) {\n        udemyImageUrl = matches[0][1];\n        break;\n      }\n    }\n  }\n  \n  // If we found a Udemy image, ensure it's the high-quality version\n  if (udemyImageUrl) {\n    // Replace low-quality versions with high-quality ones\n    udemyImageUrl = udemyImageUrl\n      .replace(/_[0-9]+x[0-9]+\\./, '_480x270.') // Ensure 480x270 resolution\n      .replace(/\\/240x135\\//, '/480x270/') // Replace low-res path\n      .replace(/\\/750x422\\//, '/480x270/'); // Replace other resolutions\n  }\n  \n  return {\n    json: {\n      ...courseData,\n      extractedUdemyImageUrl: udemyImageUrl,\n      hasUdemyImage: !!udemyImageUrl,\n      imageSource: udemyImageUrl ? 'udemy_cdn' : 'not_found',\n      discudemyPageHtml: html.substring(0, 500) + '...' // Store snippet for debugging\n    }\n  };\n});"
      },
