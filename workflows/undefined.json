    {
      "parameters": {
        "functionCode": "// Process downloaded Udemy image and prepare for storage\nconst items = $input.all();\n\nreturn items.map(item => {\n  const courseData = item.json;\n  const imageData = item.binary;\n  \n  // Get the binary data key (it's dynamic)\n  const binaryKey = Object.keys(imageData)[0];\n  \n  // Create a safe filename using course title\n  const courseTitle = courseData.title ? \n    courseData.title.replace(/^\\[100% OFF\\]\\s*/, '') // Remove [100% OFF] prefix\n      .replace(/[^a-zA-Z0-9\\s]/g, '') // Remove special characters\n      .replace(/\\s+/g, '_') // Replace spaces with underscores\n      .substring(0, 40) : \n    'udemy_course';\n  \n  const timestamp = new Date().getTime();\n  const filename = `${courseTitle}_${timestamp}.jpg`;\n  \n  return {\n    json: {\n      ...courseData,\n      downloadedImage: true,\n      localImagePath: `/course_images/${filename}`,\n      imageFilename: filename,\n      imageSize: imageData[binaryKey].data.length,\n      imageType: imageData[binaryKey].mimeType\n    },\n    binary: {\n      [binaryKey]: {\n        ...imageData[binaryKey],\n        fileName: filename\n      }\n    }\n  };\n});"
      },
